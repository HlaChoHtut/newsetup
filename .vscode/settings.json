{
	"explorer.confirmDelete": false,
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 50,
	"html.format.wrapLineLength": 100,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"editor.lineHeight": 34,
	"editor.suggestLineHeight": 20,
	"markdown.preview.lineHeight": 10,
	"debug.console.lineHeight": 20,
	"editor.fontSize": 18,
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": false,
	"workbench.colorTheme": "Imba Dark",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.editor.enablePreview": false,
	"bracketPairColorizer.depreciation-notice": false,
	"prettier.tabWidth": 4,
	"prettier.singleQuote": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"terminal.integrated.profiles.windows": {
		"Git Bash": { "path": "C:\\Program Files\\Git\\bin\\bash.exe" }
	},
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.fontSize": 14,
	"screencastMode.fontSize": 50,
	"scm.inputFontSize": 18,
	"debug.console.fontSize": 18,
	"tabnine.experimentalAutoImports": true,
	"git.decorations.enabled": false,
	"git.enabled": false,
	"javascript.validate.enable": false,
	"typescript.validate.enable": false,
	"editor.formatOnPaste": true,
	"emmet.useInlineCompletions": true,
	"emmet.triggerExpansionOnTab": true,
	"emmet.showSuggestionsAsSnippets": true,
	"editor.smoothScrolling": true,
	"editor.tabCompletion": "on",
	"files.autoSave": "onFocusChange",
	"prettier.enableDebugLogs": true,
	"prettier.jsxSingleQuote": true,
	"prettier.useTabs": true,
	"sqltools.connections": [
		{
			"previewLimit": 50,
			"server": "localhost",
			"port": 5432,
			"askForPassword": true,
			"driver": "PostgreSQL",
			"name": "postgres",
			"database": "postgres",
			"username": "postgres"
		}
	]
}
